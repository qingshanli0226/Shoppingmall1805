<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".app.MainActivity">
    <com.example.framework.view.ToolBar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:visibility="gone"
        android:layout_height="wrap_content">

    </com.example.framework.view.ToolBar>
    <com.example.framework.view.MyVP
        android:id="@+id/vp_main"
        style="@style/Base.VP.Main_VP"
        />
    <com.flyco.tablayout.CommonTabLayout
        android:background="@drawable/home_bottom_parent_bg"
        android:id="@+id/comm_main"
        app:tl_iconHeight="30dp"
        app:tl_iconWidth="30dp"
        android:paddingTop="15dp"
        app:tl_textUnselectColor="@color/MainUnSelectColor"
        app:tl_textSelectColor="@color/MainSelectColor"
        app:tl_textBold="true"
        app:tl_textsize="10sp"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        />
</LinearLayout>