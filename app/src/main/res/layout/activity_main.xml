<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".home.MainActivity">

   <android.support.v4.view.ViewPager
      style="@style/MainactivityViewPager"
       android:id="@+id/pager"
       ></android.support.v4.view.ViewPager>
      <com.flyco.tablayout.CommonTabLayout
          android:layout_width="match_parent"
          android:layout_height="@dimen/CommonTabLayout"
          app:tl_textSelectColor="@color/color_black"
          app:tl_underline_color="@color/color_black"
          android:id="@+id/com"
          app:tl_iconWidth="@dimen/CommonTabLayouticon"
          app:tl_iconHeight="@dimen/CommonTabLayouticon"
          ></com.flyco.tablayout.CommonTabLayout>
</LinearLayout>