<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shopmall.bawei.shopmall1805">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".ShopmallApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ReceliveGoodActivity"></activity>
        <activity android:name=".activity.TheorderActivity" />
        <activity android:name=".activity.location.LocationActivity" />
        <activity android:name=".activity.login.LoginActivity" />
        <activity android:name=".DetailsActivity" />
        <activity android:name=".activity.home.MainActivity" />
        <activity android:name=".activity.mesage.MessageActivity" />
        <activity android:name=".activity.WelcomActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.bawei.deom.MyServer" />
    </application>

</manifest>